   ],
   "source": [
    "with open('big.txt','r') as fd:\n",
    "    lines = fd.readlines()\n",
    "    words = []\n",
    "    for line in lines:\n",
    "        words += re.findall('\\w+', line)\n",
    "    \n",
    "print(len(words))\n",
    "word = set(words)\n",
    "print(len(word))"
   ]
